<%- include("../includes/header.ejs", {title:"Launch Pad - View All"}); %>
<h1>Launch Pad</h1>
<a href="/launch_pad/add">Add New Launch Pad</a> <br />
<br />

<table border="2">
    <tr><th>Name</th><th>Organization</th><th>Address</th><th>Actions</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><a href="/launch_pad/?launch_pad_id=<%= result[i].launch_pad_id %>"><%= result[i].pad_name %> </a></td>
        <td><a href="/organization/?organization_id=<%= result[i].organization_id %>"><%= result[i].org_name%></a></td>
        <td><a href="/location/?location_id=<%= result[i].location_id %>"><%= result[i].address%></a></td>
        <td>
            <button onclick="event.preventDefault();window.location.href='/launch_pad/edit/?launch_pad_id=<%= result[i].launch_pad_id %>'">Edit</button>
            <form action="/launch_pad/delete/">
                <input type="hidden" name="launch_pad_id" id="launch_pad_id" value="<%= result[i].launch_pad_id %>" />
                <input type="submit" value="Delete"/>
            </form>
        </td>
    </tr>
    <% } %>
</table>
<br />
<a href="/">Back to Index</a>
<%- include("../includes/footer.ejs"); %>